{
    "name": "@wulechuan/echarts-vue2-component",
    "author": "吴乐川 <wulechuan@live.com>",
    "license": "WTFPL",
    "version": "1.0.0",
    "description": "此为吴乐川的《已适配 Vuejs 2 框架的 eCharts “包裹”工具》，并支持 TypeScript 语法。",
    "main": "./源代码/发布的源代码/javascript/index.vue",
    "types": "./源代码/发布的源代码/typescripts/index.d.ts",
    "directories": {
        "source": "源代码/原始的源代码",
        "dist": "源代码/发布的源代码",
        "test": "测试集",
        "doc": "文档集"
    },
    "scripts": {
        "remove-all-html-docs": "rimraf  ./ReadMe*.html  ./文档集/说明书/**/*.html",
        "generate-html-docs": "npm  run  remove-all-html-docs    &&    wlc-md-to-html  -i ./*.md,./文档集/说明书/**/*.md  -E3  -C ./文档集/说明书/wlc-md-to-html.config.js  --to '*'",
        "检验以下代码：须发布的文件": "eslint  --fix  ./源代码/发布的源代码/**/*.vue  ./源代码/发布的源代码/**/*.js",
        "检验以下代码：原始的源文件": "eslint  --fix  ./源代码/原始的源代码/**/*.vue  ./源代码/原始的源代码/**/*.ts  ./gulpfile.esm.js/**/*",
        "检验所有代码": "npm         run  检验以下代码：原始的源文件                                         &    npm  run  检验以下代码：须发布的文件",
        "构建所有须发布的文件": "npm  run  检验以下代码：原始的源文件    &&    gulp   --silent  buildOnce    &&    npm  run  检验以下代码：须发布的文件",
        "start": "gulp  --silent",
        "每当制作集得之版本之前": "npm  i    &&    npm  run  构建所有须发布的文件",
        "prepublishOnly": "npm  run  构建所有须发布的文件"
    },
    "homepage": "https://github.com/wulechuan/wulechuan-typescript-echarts-vue2-component#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wulechuan/wulechuan-typescript-echarts-vue2-component.git"
    },
    "bugs": {
        "url": "https://github.com/wulechuan/wulechuan-typescript-echarts-vue2-component/issues"
    },
    "peerDependencies": {
        "echarts": "^5.3.2",
        "vue": "^2.6.14"
    },
    "dependencies": {
        "lodash.debounce": "^4.0.8",
        "resize-detector": "^0.3.0",
        "vue-property-decorator": "^9.1.2"
    },
    "devDependencies": {
        "@types/lodash.debounce": "^4.0.7",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "@vue/eslint-config-typescript": "^10.0.0",
        "@wulechuan/cli-scripts--git-push": "^2.0.2",
        "@wulechuan/css-stylus-markdown-themes": "^7.5.0",
        "@wulechuan/gulp-classical-task-cycle": "^0.3.0",
        "@wulechuan/vue2-sfc-from-typescript-to-javascript": "^0.5.1",
        "cssnano": "^5.1.7",
        "eslint": "^8.14.0",
        "eslint-plugin-vue": "^8.7.1",
        "esm": "^3.2.25",
        "fs-extra": "^10.1.0",
        "gulp-postcss": "^9.0.1",
        "gulp-stylus": "^2.7.1",
        "gulp-terser": "^2.1.0",
        "plugin-error": "^1.0.1",
        "rimraf": "^3.0.2",
        "stylus": "^0.57.0",
        "through2": "^4.0.2",
        "typescript": "^4.6.3"
    },
    "keywords": [
        "vue",
        "vue2",
        "vuejs",
        "echarts",
        "echart",
        "echartsjs",
        "typescript",
        "图表",
        "数据图",
        "图表绘制",
        "电子图表"
    ]
}
